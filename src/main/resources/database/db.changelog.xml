<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="schemaName" value="public"/>
    <property name="tablePrefix" value="el_"/>
    <property name="defaultTimeStamp" value="current_timestamp" dbms="mysql, postgresql, sqlite"/>
    <property name="clob" value="CLOB" dbms="sqlite"/>
    <property name="autoIncrement" value="false" dbms="mssql,mysql,postgresql"/>

    <include file="db.changelog.1.base-load.xml" relativeToChangelogFile="true"/>
    <include file="db.changelog.2.stripe.xml" relativeToChangelogFile="true"/>
    <include file="db.changelog.3.sprint1.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>